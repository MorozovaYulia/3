#include <iostream>  
using namespace std;  
int main()  
{  
    int a, b, c;  
    cout<<"vveditе А: ";  
    cin>>a;  
    cout<<"\nvveditе B: ";  
    cin>>b; 
    cout<<"\nvveditее C: ";  
    cin>>c;  
    a+=c;  
    c=a-c;  
    a-=c;  
    a+=b;  
    b=a-b;  
    a-=b; 
  cout<<"А = "<< a<<"\nB = "<<b<<"\nC = "<<c;  
    } 

 
