#include <iostream>  
using namespace std;  
int main()  
{ 
    int a, b, c;  
    cout<<"Enter А: \n ";   
    cin>>a;  
    cout<<"Enter B: \n ";  
    cin>>b;  
    cout<<"Enter C: ";  
    cin>>c;  
    a+=b;  
    b=a-b;  
    a-=b;  
    a+=c;
    c=a-c;  
    a-=c;   
    cout<<"А = "<< a<<"\n B = "<<b<<"\n C = "<<c;  
    } 
