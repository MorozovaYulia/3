#include <iostream>  
using namespace std;  
int main()  
{ 
    int a, b, c;  
    cout<<"Enter a: \n ";   
    cin>>a;  
    cout<<"Enter b: \n ";  
    cin>>b;  
    cout<<"Enter c: ";  
    cin>>c;  
    a+=b;  
    b=a-b;  
    a-=b;  
    a+=c;
    c=a-c;  
    a-=c;   
    cout<<"a = "<< a<<"\n b = "<<b<<"\n c = "<<c;  
    } 
