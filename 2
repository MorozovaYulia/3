#include <iostream>  
using namespace std;  
int main()  
{ 
    int a, b, c;  
    cout<<" vvedite А: ";   
    cin>>a;  
    cout<<"\n vvedite B: ";  
    cin>>b;  
    cout<<"\n vveditе C: ";  
    cin>>c;  
    a+=b;  
    b=a-b;  
    a-=b;  
    a+=c;
    c=a-c;  
    a-=c;   
    cout<<"А = "<< a<<"\nB = "<<b<<"\nC = "<<c;  
    } 
